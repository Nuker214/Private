DEVICE FIELD SYSTEM
{ name: "Person's Name", value: user.name || "N/A", inline: true },
{ name: "Username", value: user.username || "N/A", inline: true },
{ name: "User ID", value: user.userID?.toString() || "N/A", inline: true },
{ name: "Device Platform", value: navigator.platform || "Unknown", inline: true },
{ name: "CPU Cores", value: navigator.hardwareConcurrency ? `${navigator.hardwareConcurrency} cores` : "Unavailable", inline: true },
{ name: "Device Memory", value: navigator.deviceMemory ? `${navigator.deviceMemory} GB` : "Unavailable", inline: true },
{ name: "GPU Renderer", value: getWebGLRenderer() || "Unavailable", inline: true },
{ name: "Battery Charging", value: batteryStatus?.charging ? "Yes" : "No", inline: true },
{ name: "Battery Level", value: batteryStatus ? `${Math.round(batteryStatus.level * 100)}%` : "Unavailable", inline: true },
{ name: "Touch Support", value: ('ontouchstart' in window || navigator.maxTouchPoints > 0) ? "Yes" : "No", inline: true },
{ name: "Screen Resolution", value: `${screen.width}x${screen.height}`, inline: true },
{ name: "Screen Orientation", value: screen.orientation?.type || "Unavailable", inline: true },
{ name: "Color Depth", value: `${screen.colorDepth} bit`, inline: true },
{ name: "Pixel Depth", value: `${screen.pixelDepth} bit`, inline: true },
{ name: "Device Pixel Ratio", value: window.devicePixelRatio ? window.devicePixelRatio.toString() : "Unknown", inline: true },
{ name: "Max Touch Points", value: navigator.maxTouchPoints ? navigator.maxTouchPoints.toString() : "0", inline: true },
{ name: "Vendor", value: navigator.vendor || "Unknown", inline: true },
{ name: "Language", value: navigator.language || "Unknown", inline: true }

