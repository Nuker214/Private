BROWSER FIELD SYSTEM
{ name: "Person's Name", value: user.name || "N/A", inline: true },
{ name: "Username", value: user.username || "N/A", inline: true },
{ name: "User Identifier", value: user.userID?.toString() || "N/A", inline: true },
{ name: "Browser Vendor", value: navigator.vendor || "Unknown", inline: true },
{ name: "Browser Language", value: navigator.language || "Unknown", inline: true },
{ name: "Browser Languages", value: navigator.languages ? navigator.languages.join(", ") : "Unknown", inline: false },
{ name: "User Agent", value: navigator.userAgent || "Unknown", inline: false },
{ name: "Installed Plugins", value: navigator.plugins?.length > 0 ? `${navigator.plugins.length} plugin(s)` : "None", inline: true },
{ name: "Browser Window Size", value: `${window.innerWidth}x${window.innerHeight}`, inline: true },
{ name: "Browser Zoom Level", value: `${Math.round(window.devicePixelRatio * 100)}%`, inline: true },
{ name: "Incognito/Private Mode Status", value: "Undetected", inline: true },
{ name: "Cookies Enabled", value: navigator.cookieEnabled ? "Yes" : "No", inline: true },
{ name: "Do Not Track", value: navigator.doNotTrack || "Unknown", inline: true },
{ name: "Browser Tab Visibility", value: document.visibilityState || "Unknown", inline: true },
{ name: "Font Smoothing Type", value: "Unavailable", inline: true },
{ name: "Clipboard Access", value: navigator.clipboard ? "Supported" : "Not Supported", inline: true },
{ name: "WebGL Renderer Info", value: "Unavailable", inline: true },
{ name: "Browser Connection Speed", value: navigator.connection?.effectiveType || "Unknown", inline: true }

